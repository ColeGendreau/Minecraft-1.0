# Example Terraform variables file
# Copy this file to terraform.tfvars and customize as needed

# Project configuration
project_name = "mc-demo"
environment  = "dev"
location     = "westus3"

# AKS configuration
aks_node_count     = 2
aks_node_size      = "Standard_D2as_v5"
kubernetes_version = "1.28"

# Container Registry
acr_sku = "Basic"

# Tags
tags = {
  Project     = "Minecraft-DevOps"
  ManagedBy   = "Terraform"
  Environment = "dev"
  Owner       = "YourName"
}

